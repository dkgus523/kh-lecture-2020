<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.github.hyeyoom.springbootonintellij.repository.UserRepository">
    <insert id="createUser" parameterType="com.github.hyeyoom.springbootonintellij.domain.User">
        INSERT INTO users(name, email) VALUES (#{name}, #{email})
    </insert>
</mapper>